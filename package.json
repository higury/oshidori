{
  "name": "oshidori",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint --bailOnLintError true",
    "repair": "/bin/bash -c 'ionic repair && ionic cordova platform add ios android browser'",
    "ios:device": "ionic cordova run ios -lc --device -- --buildFlag='-UseModernBuildSystem=0'",
    "ios:emu:live": "ionic cordova run ios -lc --emulator -- --buildFlag='-UseModernBuildSystem=0'",
    "ios:emu": "ionic cordova run ios --emulator -- --buildFlag='-UseModernBuildSystem=0'",
    "android:emu": "/bin/bash -c 'ionic cordova run android -lc --emulator -- --buildFlag=\"-UseModernBuildSystem=0\"'"
  },
  "dependencies": {
    "@angular/common": "5.0.0",
    "@angular/compiler": "5.0.0",
    "@angular/compiler-cli": "5.0.0",
    "@angular/core": "5.0.0",
    "@angular/fire": "^5.1.1",
    "@angular/forms": "5.0.0",
    "@angular/http": "5.0.0",
    "@angular/platform-browser": "5.0.0",
    "@angular/platform-browser-dynamic": "5.0.0",
    "@ionic-native/barcode-scanner": "^4.20.0",
    "@ionic-native/camera": "^4.19.0",
    "@ionic-native/core": "^4.19.0",
    "@ionic-native/file": "^4.19.0",
    "@ionic-native/image-picker": "^3.14.0",
    "@ionic-native/photo-library": "^4.19.0",
    "@ionic-native/splash-screen": "3.12.1",
    "@ionic-native/status-bar": "3.12.1",
    "@ionic/storage": "^2.2.0",
    "@types/uuid": "^3.4.4",
    "angular2-uuid": "^1.1.1",
    "com.synconset.imagepicker": "^2.1.10",
    "cordova-android": "7.1.4",
    "cordova-browser": "5.0.4",
    "cordova-ios": "4.5.5",
    "cordova-plugin-add-swift-support": "^1.7.2",
    "cordova-plugin-camera": "^4.0.3",
    "cordova-plugin-console": "^1.1.0",
    "cordova-plugin-device": "^2.0.2",
    "cordova-plugin-file": "^6.0.1",
    "cordova-plugin-splashscreen": "^5.0.2",
    "cordova-plugin-statusbar": "^2.4.2",
    "cordova-plugin-telerik-imagepicker": "^2.2.3",
    "cordova-plugin-whitelist": "^1.3.3",
    "cordova-sqlite-storage": "^2.6.0",
    "firebase": "^5.8.0",
    "ionic-angular": "3.9.2",
    "ionic-image-loader": "^6.3.2",
    "ionic-plugin-keyboard": "^2.2.1",
    "ionicons": "3.0.0",
