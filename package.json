    "qrcode": "^1.3.3",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.3",
    "spinkit": "^1.2.5",
    "sw-toolbox": "3.6.0",
    "zone.js": "0.8.27"
  },
  "devDependencies": {
    "@ionic/app-scripts": "3.2.2",
    "@types/node": "^10.12.18",
    "tslint-ionic-rules": "0.0.21",
    "typescript": "^2.7.2"
  },
  "description": "An Ionic project",
  "cordova": {
    "plugins": {
      "cordova-plugin-console": {},
      "cordova-plugin-device": {},
      "cordova-plugin-splashscreen": {},
